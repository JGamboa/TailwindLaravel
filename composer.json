{
    "name": "tomjamon/tailwindlaravel",
    "description": "Form Input based on tailwind & laravelcollective",
    "type": "library",
    "authors": [
        {
            "name": "Tom Jamon",
            "email": "contact@tomjamon.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/http": "5.7.*",
        "illuminate/routing": "5.7.*",
        "illuminate/session": "5.7.*",
        "illuminate/support": "5.7.*",
        "illuminate/view": "5.7.*"
    },
    "require-dev": {
        "illuminate/database": "5.7.*",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.1"
    },
    "autoload": {
        "psr-4" : {
            "TomJamon\\TailwindLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomJamon\\TailwindLaravel\\TailwindLaravelServiceProvider"
            ],
            "aliases": {
                "TailwindForm": "TomJamon\\TailwindLaravel\\TailwindLaravelFormFacade",
                "TailwindHtml": "TomJamon\\TailwindLaravel\\TailwindLaravelHtmlFacade"
            }
        }
    }
}
